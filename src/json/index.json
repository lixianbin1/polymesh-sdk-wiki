[
  {
    "name":"accountManagement",
    "docs":"账号管理",
    "type":"attribute",
    "children":[
      {
        "name":"getAccount",
        "type":"method",
        "docs":"通过地址获取账号实例,如果帐户具有多重签名者，则返回的值将是多重签名实例",
        "parameters":[{
          "name":"address",
          "type":"mandatory",
          "docs":"请输入SS58格式地址"
        }]
      },{
        "name":"getAccountBalance",
        "type":"method",
        "docs":"获取账户的可用/锁定 POLYX 余额",
        "parameters":[{
          "name":"account",
          "type":"optional",
          "docs":"请选择账户"
        }]
      },{
        "name":"getSigningAccount",
        "type":"method",
        "docs":"获取当前签名帐户，如果未设置签名帐户，则返回 null",
        "parameters":[]
      },{
        "name":"getSigningAccounts",
        "type":"method",
        "docs":"获取当前的签名管理器中所有的账户",
        "parameters":[]
      },{
        "name":"inviteAccount",
        "type":"method",
        "docs":"发送邀请辅助账户的邀请",
        "parameters":[{
          "name":"targetAccount",
          "type":"mandatory",
          "docs":"请输入被邀请的账号的SS58格式地址"
        },{
          "name":"expiry",
          "type":"optional",
          "docs":"请选择过期时间"
        },{
          "name":"permissions",
          "type":"optional",
          "docs":"请设置被邀请人权限"
        }]
      }
    ]
  },
  {
    "name":"assets",
    "docs":"资产",
    "type":"attribute",
    "children":[
      {
        "name":"getAsset",
        "type":"method",
        "docs":"根据资产名称获取资产实例",
        "parameters":[{
          "name":"ticker",
          "type":"mandatory",
          "docs":"请输入资产名称"
        }]
      }
    ]
  },
  {
    "name":"claims",
    "docs":"声明",
    "type":"attribute",
    "children":[
      {
        "name":"getCddClaims",
        "type":"method",
        "docs":"根据身份标识获取CDD列表",
        "parameters":[{
          "name":"includeExpired",
          "type":"optional",
          "docs":"是否包括过期的声明"
        },{
          "name":"target",
          "type":"optional",
          "docs":"要为获取CDD声明的身份"
        }]
      }
    ]
  },
  {
    "name":"identities",
    "docs":"身份标识",
    "type":"attribute",
    "children":[
      {
        "name":"getIdentity",
        "type":"method",
        "docs":"根据did获取身份实例",
        "parameters":[{
          "name":"did",
          "type":"mandatory",
          "docs":"身份did"
        }]
      }
    ]
  },
  {
    "name":"network",
    "docs":"区块链网络",
    "type":"attribute",
    "children":[
      {
        "name":"getLatestBlock",
        "type":"method",
        "docs":"获取区块链上的最新区块的编号",
        "parameters":[]
      }
    ]
  },
  {
    "name":"settlements",
    "docs":"场地标识",
    "type":"attribute",
    "children":[
      {
        "name":"getVenue",
        "type":"method",
        "docs":"根据场地标识符获取场地实例",
        "parameters":[{
          "name":"id",
          "type":"mandatory",
          "docs":"场地标识符",
          "datetype":"BigNumber"
        }]
      }
    ]
  }
]